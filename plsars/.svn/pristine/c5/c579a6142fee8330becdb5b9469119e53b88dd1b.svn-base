package com.asj.pls.info;

import java.io.Serializable;

/**
 * 订单实体类
 * */
public class OrdersInfo implements Serializable {
	
	private static final long serialVersionUID = -7452348332582739004L;
	
	private String ordersNo;//订单编号
	private String time;//下单时间
	private String price;//应付金额
	private String status;//订单状态
	private Integer value;//状态值
	
	public String getOrdersNo() {
		return ordersNo;
	}
	public void setOrdersNo(String ordersNo) {
		this.ordersNo = ordersNo;
	}
	public String getTime() {
		return time;
	}
	public void setTime(String time) {
		this.time = time;
	}
	public String getPrice() {
		return price;
	}
	public void setPrice(String price) {
		this.price = price;
	}
	public String getStatus() {
		return status;
	}
	public void setStatus(String status) {
		this.status = status;
	}
	public Integer getValue() {
		return value;
	}
	public void setValue(Integer value) {
		this.value = value;
	}
}
